{
  "name": "mgtranslate-media-api",
  "version": "1.0.0",
  "description": "Google Meet Media API client for real-time audio capture",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npm run build:sdk && tsc",
    "build:sdk": "webpack --config webpack.config.cjs",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.4.1",
    "googleapis": "^129.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "ts-loader": "^9.5.4",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  }
}
